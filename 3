def ler_valores_distintos():
    vetor = []
    while len(vetor) < 10:
        valor = int(input(f"Digite o {len(vetor) + 1}º valor distinto: "))
        if valor not in vetor:
            vetor.append(valor)
        else:
            print("Valor já inserido, por favor digite um valor distinto.")
    return vetor

def obter_pares_com_posicoes(vetor):
    return [(i, num) for i, num in enumerate(vetor) if num % 2 == 0]

vetor = ler_valores_distintos()

pares_com_posicoes = obter_pares_com_posicoes(vetor)

print("Valores pares e suas posições:")
for posicao, valor in pares_com_posicoes:
    print(f"Posição {posicao}: {valor}")
